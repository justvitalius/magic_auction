# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
/public/assets
/public/system/*
/public/uploads/*
/public/sitemap*

/db/schema.rb

# Ignore IDE files
/.idea
/.rvmrc
/.sass_cache
/.system
.zeus.sock

